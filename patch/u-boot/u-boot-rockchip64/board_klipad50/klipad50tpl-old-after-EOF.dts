&sdio {
		pinctrl-names = "default";
		//required
		pinctrl-0 = <&sdmmc1_bus4 &sdmmc1_cmd &sdmmc1_clk>;
		mmc-pwrseq = <&sdio_pwrseq0>;
		status = "okay";
		non-removable;
		bus-width = <0x04>;
		//optional
		cap-sd-highspeed;
		cap-sdio-irq;
};
//---kernel-end---
//tm end

